---
import BaseLayout from "../layouts/BaseLayout.astro";
import BaseCard from "../components/BaseCard.astro";
import TimeLine from "../components/page/TimeLine.astro";
---

<!doctype html>
<BaseLayout PageID="about">
  <BaseCard title="About Frosti">
    <h1 id="h1">Habiliades</h1>
   <h2 id="h2">Tecnologías utilizadas</h2>
    <h5 id="h2">Frameworks Backend</h5>
    <ul class="timeline timeline-vertical">
      <li>
        <TimeLine time="Mar 17, 2019" left="true" check="true">
          <a href="https://laravel.com" target="_blank">Laravel</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-primary" />
        <TimeLine time="Apr 13, 2022" left="true" check="true">
          <a href="https://expressjs.com" target="_blank">Express</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-primary" />
        <TimeLine time="Fer 01, 2019" left="true" check="true">
          <a href="https://nestjs.com" target="_blank">NestJS</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-primary" />
        <TimeLine time="Dec 19, 2020" left="true" check="true">
          <a
            href="https://dotnet.microsoft.com/es-es/apps/aspnet"
            target="_blank">ASP.NET</a
          ></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-neutral-content" />
        <TimeLine time="WAITING"> Esperando...</TimeLine>
      </li>
    </ul>
    <h5 id="h2">Frameworks Frontend</h5>
    <ul class="timeline timeline-vertical">
      <li>
        <TimeLine time="Jan 02, 2020" left="true" check="true">
          <a href="https://reactjs.org" target="_blank">React</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>

      <li>
        <hr class="bg-primary" />
        <TimeLine time="Dec 06, 2021" left="true" check="true">
          <a href="https://vuejs.org" target="_blank">Vue</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-primary" />
        <TimeLine time="Apr 13, 2020" left="true" check="true">
          <a href="https://blade-ui-kit.com" target="_blank">Blade</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-primary" />
        <TimeLine time="May 16, 2023" left="true" check="true">
          <a href="https://nextjs.org" target="_blank">NextJS</a></TimeLine
        >
        <hr class="bg-primary" />
      </li>
      <li>
        <hr class="bg-neutral-content" />
